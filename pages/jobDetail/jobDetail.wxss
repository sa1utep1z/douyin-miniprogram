  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  padding: 0 40rpx;
  border-top: 1rpx solid #D8D8D8;
}

.float-column {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.float-button {
  width: 380rpx;
  height: 92rpx;
  /* background: linear-gradient(167deg, #36E8FE 0%, #1283E9 100%); */
  background: linear-gradient(#018eff, #51bcff);
  border-radius: 10rpx;
  font-size: 36rpx;
  text-align: center;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #FFFFFF;
  line-height: 92rpx;
}
.float-button-hover {
