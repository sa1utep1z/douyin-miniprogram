}
.address-title {
  height: 44rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: #333333;
  line-height: 44rpx;
}
.address-row {
  width: 100%;
  display: flex;
  align-items: center;
  flex-direction: row;
}
.address-column{
  flex: 1;
  margin-top: 16rpx;
}
.address-content {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #333333;
  line-height: 40rpx;
}
