/* pages/index/jobDetail/jobDetail.wxss */
page{
  width: 100%;
  height: 100%;
  background-color: #f8f8f8;
  overflow: hidden;
}
.container {
  width: 100%;
  display: flex;
  padding-bottom: 142rpx;
  flex-direction: column;
  box-sizing: border-box;
  align-items: center;
  position: relative;
}
.box {
  width: 100%;
  height: 100%;
  overflow: scroll;
}
.box::-webkit-scrollbar{
  display:none;
}
.swipe-view {
  width: 100%;
  height: 432rpx;
}
.swiper-item {
  width: 100%;
 height: 432rpx !important;
}
.job-view{
  width: 750rpx;
  background: #FFFFFF;
  border-radius: 12rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
  margin-bottom: 2rpx;
}
.job-price-row {
  width: 100%;
  display: flex;
  padding: 20rpx 32rpx;
  box-sizing: border-box;
  flex-direction: row;
  align-items: center;
}
.job-tip {
  height: 40rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #999999;
  line-height: 40rpx;
}
.job-price {
  text-align: left;
  height: 48rpx;
  flex: 1;
  font-size: 40rpx;
  font-family: Roboto-Bold, Roboto;
  font-weight: bold;
  color: #FF830B;
  line-height: 48rpx;
  box-sizing: border-box;
  padding-left: 20rpx;
}
.job-tag-row {
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-start;
  padding-left: 24rpx;
  padding-bottom: 20rpx;
  box-sizing: border-box;
  margin-top: 12rpx;
}
.job-tag-row-block {
  width: 100%;
  display: block;
  align-items: center;
  padding-left: 24rpx;
  padding-bottom: 20rpx;
  box-sizing: border-box;
  margin-top: 12rpx;
}
.job-type-tag {
  height: 34rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #67B0F2;
  line-height: 34rpx;
  padding: 0 8rpx;
  background: #FFFFFF;
  border-radius: 2rpx;
  margin-right: 8rpx;
  border: 2rpx solid #67B0F2;
}
.job-des-tag {
  height: 34rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #999999;
  line-height: 34rpx;
  margin-left: 12rpx;
}
.job-tag {
  height: 34rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  padding: 0 8rpx;
  background: #F5F5F5;
  color: #666666;
  line-height: 34rpx;
  border-radius: 4rpx;
  margin-left: 12rpx;
}
.job-tag-block {
  display: block;
  float: left;
  height: 34rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  padding: 0 8rpx;
  background: #E1F2FF;
  color: #018EFF;
  line-height: 34rpx;
  border-radius: 4rpx;
  margin-right: 12rpx;
  margin-bottom: 12rpx;
}
.job-title {
  width: 100%;
  font-size: 36rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: #333333;
  line-height: 50rpx;
  margin-top: 24rpx;
  padding: 0 24rpx;
  box-sizing: border-box;
}
.job-action-row {
  width: 100%;
  height: 82rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.job-des {
  flex: 1;
  height: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.job-des-text {
  height: 34rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #999999;
  line-height: 34rpx;
  margin-left: 6rpx;
}
.job-des-icon {
  width: 31rpx;
  height: 29rpx;
}
.job-divider {
  width: 2rpx;
  height: 82rpx;
  background: #F8F8F8;
}
.van-divider {
  margin: 0 !important;
  height: 2rpx !important;
}
.horizontal-divider {
  width: 100%;
  height: 2rpx;
  background-color: #f8f8f8;
  margin-top: 24rpx;
}

.manager-view {
  width: 750rpx;
  height: 148rpx;
  background: #FFFFFF;
  margin-bottom: 2rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 24rpx;
  box-sizing: border-box;
}
.manager-avatar {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}
.manager-column {
  flex: 1;
  margin-left: 16rpx;
}
.manager-name {
  height: 40rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #333333;
  line-height: 40rpx;
}
.manager-company {
  height: 34rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #999999;
  line-height: 34rpx;
}
.manager-phone {
  width: 132rpx;
  height: 46rpx;
  text-align: center;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #018EFF;
  line-height: 46rpx;
  border-radius: 4rpx;
  box-sizing: border-box;
  border: 2rpx solid #018EFF;
}
.manager-wechat {
  width: 132rpx;
  height: 46rpx;
  font-size: 24rpx;
  text-align: center;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #FFFFFF;
  line-height: 46rpx;
  background: #018EFF;
