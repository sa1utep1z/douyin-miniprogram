/* pages/member/member.wxss */
page{
  width: 100%;
  height: 100%;
  background-color:#FFFFFF;
}
.member-container {
  width: 100%;
  background: #FFFFFF;
}
/* tab样式 开始 */
.tab-view {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 72rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 0 30rpx;
  margin-bottom: 24rpx;
  margin-top: 40rpx;
  box-sizing: border-box;
  justify-content: space-between;
}
.tab-item {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.tab-item-title {
  height: 44rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  line-height: 44rpx;
}
.tab-title-normal {
  color: #333333;
}
.tab-title-active {
  color: #018EFF;
  font-weight: bolder;
}
/* tab样式 结束 */

.scroll-view {
  position: fixed;
  top: 130rpx;
  left: 0;
  width: 100%;
  max-height: 1600rpx;
}

.status-loading {
  width: 100%;
  padding: 24rpx 30rpx;
  text-align: center;
}
.status-finish {
  width: 100% ;
  display: block;
  padding: 30rpx 30rpx;
  box-sizing: border-box;
}
.status-finish .van-divider {
  color: #999999 !important;
  width: 100% !important;
  font-size: 24rpx !important;
  margin: 0 !important;
}
.van-divider-container {
  width: 100%;
  display: block;
}
.recommend-view {
  width: 686rpx;
  background: #FFFFFF;
  padding:24rpx;
  box-sizing: border-box;
  border-radius: 12rpx;
  display: flex;
  flex-direction: column;
  margin: 32rpx 32rpx 0 32rpx;

}
.recommend-title {
  height: 44rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: #333333;
  line-height: 44rpx;
}
.recommend-row {
  width: 100%;
  display: flex;
  flex-direction: row;
  margin-top: 24rpx;
  align-items: center;
  justify-content: space-between;
}
.member-phone {
  height: 40rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  line-height: 40rpx;
  color: #018EFF;
}

.member-view {
  background-color: #f8f8f8;
  box-sizing: border-box;
  border-radius: 12rpx;
  display: flex;
  flex-direction: column;
  padding: 32rpx 32rpx 0 32rpx;
  margin-bottom: 20rpx;
  align-items: center;
}
.member-row {
  width: 100%;
  height: 60rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}
.member-text {
  height: 40rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #333333;
  line-height: 40rpx;
}
.van-divider {
  width: 100% !important;
  margin: 0!important;
  border-color: #f8f8f8 !important;
}


.container {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  background: rgba(0,0,0,0.5);
}
.dialog {
  width: 640rpx;
  background: white;
  border-radius: 12px;
  box-sizing: border-box;
}
.dialog-content {
  padding: 0 48rpx;
  width: 100%;
  display: flex;
  box-sizing: border-box;
  flex-direction: column;
  align-items: center;
}
.dialog-title {
  width: 544rpx;
  height: 50rpx;
  font-size: 36rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #333333;
  text-align: center;
  margin-top: 64rpx;
  line-height: 50rpx;
}
.input-view-required {
  width: 544rpx;
  height: 80rpx;
  background: #F8F8F8;
  border-radius: 4rpx;
  display: flex;
  flex-direction: row;
  margin-top: 32rpx;
  align-items: center;
  padding: 20rpx 24rpx;
  box-sizing: border-box;
}
.input-view-label {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  line-height: 40rpx;
}
.van-divider {
  height: 2rpx;
  width: 100%;
  background-color: #f8f8f8;
  margin-top: 64rpx;
}
.btn-row {
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}
.btn-cancel{
  width: 288rpx;
  height: 110rpx;
  font-size: 34rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #333333;
  text-align: center;
  line-height: 110rpx;
  letter-spacing: 2rpx;
}
.btn-confirm{
  width: 288rpx;
  height: 110rpx;
  font-size: 34rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #018EFF;
  line-height: 110rpx;
  text-align: center;
  letter-spacing: 2rpx;
}
.divider {
  height: 110rpx;
  width: 2rpx;
  background-color: #f8f8f8;
}
.edit-return-visit-switch {
  margin-left: 40rpx;
}